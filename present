#include <stdio.h>
	
	
void present(){
	int linha, coluna;
	char present[16][45] ={{"\t  ---------------------------------------- \n"},
                         {"\t  |###      ####      ### ##### #######  | \n"},
                         {"\t  | ###    ######    ### ### ## ###  ### | \n"},
                         {"\t  |  ###  ###  ###  ### ####### #######  | \n"},
                         {"\t  |   ######    ###### #### ### ### ###  | \n"},
                         {"\t  |    ####      #### ####  ### ###  ### | \n"},
                         {"\t  |                                      | \n"},
                         {"\t  |   #####       ### ###         #######| \n"},
                         {"\t  |  #######     #### ####      #########| \n"},
                         {"\t  | ###   ###   ##### #####    #####     | \n"},
                         {"\t  | ##         ### ## ######  ###########| \n"},
                         {"\t  | ###  #### ####### ### ###### ###     | \n"},
                         {"\t  |  ###  ## ###  ### ###  ####  ########| \n"},
                         {"\t  |   ##### ###   ### ###         #######| \n"},
                         {"\t  ---------------------------------------- \n"}};
						
	for(linha=0;linha<15;linha++){
	for(coluna=0; coluna<45;coluna++){
		printf("%c", present[linha][coluna]);
		Sleep(15);
		}
	}
}	
	
	
int main(){
	present();

	return 0;
}
